    "def linearsearch(A, key):\n",
    "    index = 0\n",
    "    while index < len(A):\n",
    "        if A[index] == key :\n",
    "            return index\n",
    "        index = index + 1\n",
    "    return -1\n",
    "\n",
    "A = [84, 21, 47, 96, 15]\n",
    "found = linearsearch(A,96)\n",
    "print('Result:',found)"
